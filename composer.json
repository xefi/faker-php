{
  "name": "xefi/faker-php",
  "type": "library",
  "description": "Faker allows you to generate realistic fake data for your php applications",
  "keywords": [
    "faker",
    "data",
    "seeding",
    "fixtures"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Gautier Deleglise"
    },
    {
      "name": "Martin Soenen"
    }
  ],
  "require": {
    "php": "^8.3",
    "phpdocumentor/reflection-docblock": "^5.6",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "Xefi\\Faker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xefi\\Faker\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "faker": {
      "providers": [
        "Xefi\\Faker\\FakerServiceProvider"
      ]
    }
  }
}
